import java.util.*;
 class student {
String name;
int rollno;
int []marks=new int[5];
void getdata()
    {Scanner obj=new Scanner(System.in);
    System.out.println("Enter name:");
    name=obj.nextLine();
    System.out.println("\nEnter rollno:");
    
    rollno=obj.nextInt();
    
    
    System.out.println("\nEnter marks for 5 subjects:");
    for(int i=0;i<5;i++)
    {marks[i]=obj.nextInt();
    }}
    void putdata()
    {System.out.println("Name:");
    System.out.println(name);
    System.out.println("\nRollNo:");
    System.out.println(rollno);
    System.out.println("\nMarks for 5 subjects:");
    for(int i=0;i<5;i++)
    {System.out.println(marks[i]);
    }


}


public static void main(String[] args)
{

    student stu=new student();
    stu.getdata();
    stu.putdata();
    
    }
    }

